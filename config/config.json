{
  "settings": {
    "download_dir": "/some folder/",
    "data_dir": "/some other folder/.data",
    "filename": "%episode_date%-%episode_url_basename%%episode_url_extension%",
    "tags": [
      {
        "replace": "title",
        "with": "%episode_title%"
      },
      {
        "replace": "artist",
        "with": "%episode_author%"
      },
      {
        "replace": "album",
        "with": "%feed_title%"
      },
      {
        "replace": "date",
        "with": "%episode_date%"
      },
      {
        "replace": "website",
        "with": "%episode_link%"
      }
    ]
  },
  "feeds": [
    {
      "name": "Pseudopod",
      "url": "https://pseudopod.org/feed/podcast/"
    }
  ]
}
